#include <iostream>
#include <fstream>
#include <time.h>
#include <bits/stdc++.h>
#include <cstdlib>
using namespace std;

void timeframe(double second){
    time_t starttime=time(0);
    while(true){
    time_t endtime=time(0);
    if (difftime(endtime,starttime)>=second){
      break;
    }
    }
}
int main ()  {
  
bool login_success = false;
  
string iconstr,iconstra;

ifstream icon("icon.txt"),iconintrofull("icon1.txt");

  if (login_success==false){
        
        for(int i=0;i<10;i++){
            getline(icon,iconstr);
            cout<<iconstr<<endl;
        }
            timeframe(2);
		for(int i=0;i<2;i++){
        getline(iconintrofull,iconstra);
            cout<<iconstra<<endl;
        }
    login_success==true;
}
return 0;

}
